<mapper namespace="com.aia.op.mybatis.mapper.MemberMapper.MemberDaoInterface">
	
	<!--
		1. id = 이름작성 
		2. sql문 작성
		3. 파라미터 정의
	-->
	
	<select id="selectById"
		resultType="com.aia.op.member.model.Member">
		select count(*) from project.member where uid = #{uid}		
	</select>
	
</mapper>